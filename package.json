{
  "name": "react-library-template",
  "description": "",
  "version": "1.0.0",
  "main": "dist/bundle.js",
  "typings": "dist/index.d.ts",
  "author": "Origami ONOUE <onoueorigami@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "test": "jest --config=jest/test/jest.config.js",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:visual": "jest --config=jest/integration/jest.config.js",
    "test:visual:update": "yarn test:visual -u",
    "test:visual-integration": "start-server-and-test storybook http-get://localhost:6006 test:visual",
    "test:visual-integration:update": "start-server-and-test storybook http-get://localhost:6006 test:visual:update",
    "typecheck": "node scripts/typecheck.js",
    "typecheck:watch": "tsc --watch",
    "packing:prod": "webpack --mode=production",
    "packing:dev": "webpack --mode=development",
    "build": "cross-env NODE_ENV=production run-p -l -n -c packing:prod typecheck",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o public",
    "lint:ts": "eslint ./src/**/*.ts ./src/**/*.tsx  --cache",
    "lint:js": "eslint ./*.js --cache",
    "lint:json": "prettier \"*.json\"",
    "lint:style": "stylelint \"./src/**/*.tsx\" --syntax css-in-js",
    "lint:fix:ts": "yarn lint:ts --fix",
    "lint:fix:js": "yarn lint:js --fix",
    "lint:fix:json": "yarn lint:json --write",
    "lint:fix:style": "yarn lint:style --fix",
    "lint": "run-p -l -n -c --aggregate-output lint:*",
    "lint:fix": "run-p -l -n -c --aggregate-output lint:fix:*"
  },
  "devDependencies": {
